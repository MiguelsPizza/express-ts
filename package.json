{
  "name": "typed-router",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "turbo run dev --filter=@typed-router/react-spa --filter=@typed-router/express-server",
    "build": "turbo run build --filter=!@typed-router/e2e-tests",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "typecheck": "turbo run typecheck",
    "test:e2e:dev": "pnpm --filter @typed-router/e2e-tests test:e2e:ci"
  },
  "engines": {
    "node": ">=22.12",
    "pnpm": "^10.0.0"
  },
  "devDependencies": {
    "@typed-router/prettier-config": "workspace:*",
    "prettier": "catalog:",
    "eslint": "catalog:",
    "jiti": "^2.4.2",
    "tsup": "catalog:",
    "tsx": "4.19.2",
    "typescript": "catalog:"
  },
  "prettier": "@typed-router/prettier-config",
  "dependencies": {
    "dotenv": "^16.4.5",
    "pnpm": "10.0.0",
    "tslib": "^2.0.0",
    "turbo": "^2.3.3"
  }
}
