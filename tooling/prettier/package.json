{
  "name": "@typed-router/prettier-config",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "node": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "prettier": "catalog:",
    "tsup":  "catalog:"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@typed-router/tsconfig": "workspace:*",
    "@typed-router/tsup-config": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@typed-router/prettier-config"
}
