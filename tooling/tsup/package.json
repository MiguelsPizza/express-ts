{
  "name": "@express-ts/tsup-config",
  "description": "Shared tsup configuration to be used across packages. Use this for building internal libs",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tsup": "catalog:"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@express-ts/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@express-ts/prettier-config"
}
