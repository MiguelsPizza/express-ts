{
  "name": "@express-ts/e2e-tests",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "prep": "pnpm lint-staged",
    "clean": "git clean -xdf .cache .output test-results playwright-report .turbo node_modules",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../.gitignore",
    "typecheck": "tsc --noEmit",
    "e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test": "playwright test --reporter=list"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@playwright/test": "^1.46.1",
    "@express-ts/eslint-config": "workspace:*",
    "@express-ts/prettier-config": "workspace:*",
    "@express-ts/tsconfig": "workspace:*",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "dotenv": "^16.4.5",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "^18.3.1",
    "typescript": "catalog:",
    "pnpm": "10.0.0"
  },
  "prettier": "@express-ts/prettier-config",
  "dependencies": {
    "vitest": "^2.0.5"
  }
}