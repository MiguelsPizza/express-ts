{
  "name": "@typed-router/express-server",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "exports": {
    ".": {
      "default": "./src/routes/apiRoutes.ts"
    }
  },
  "scripts": {
    "start": "node dist/server.cjs",
    "dev": "tsx watch ./server.ts",
    "dev-debug": "tsx watch --inspect ./server.ts",
    "build": "tsup-node --tsconfig ./tsconfig.json",
    "test": "vitest --coverage --ui --coverage.enabled=true",
    "prep": "pnpm lint-staged",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "clean": "git clean -xdf .cache dist .coverage .turbo node_modules",
    "lint": "node --max-old-space-size=8192 ../../node_modules/eslint/bin/eslint.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "mongoose": "catalog:",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2"
  },
  "devDependencies": {
    "@typed-router/router": "workspace:*",
    "@typed-router/shared-lib": "workspace:*",
    "@typed-router/tsup-config": "workspace:*",
    "@types/compression": "^1.7.5",
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@typed-router/eslint-config": "workspace:*",
    "@typed-router/prettier-config": "workspace:*",
    "@typed-router/tsconfig": "workspace:*",
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.1",
    "mongodb-memory-server": "^9.1.8",
    "prettier": "catalog:",
    "tsx": "^4.7.2",
    "typescript": "catalog:",
    "vitest": "^2.0.5",
    "tsup": "catalog:"
  },
  "prettier": "@typed-router/prettier-config"
}
